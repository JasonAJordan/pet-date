<%= render 'shared/errors' %>

<%= form_for @user do |f| %>

  <%= f.label :user_name %>
  <%= f.text_field :user_name %><br><br>

  <%= f.label :password %>
  <%= f.password_field :password %><br><br>

  <%= f.label :name %>
  <%= f.text_field :name %><br><br>

  <%= f.label :bio %>
  <%= f.text_field :bio %><br><br>

  <%= f. label "Profile Photo" %>
  <%= f.file_field :pictures, multiple: true %><br><br>


  <%= f.submit %>
<% end %>