<%= render 'shared/errors' %>

<%= form_for @location do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br><br>

  <%= f.label :neighbourhood %>
  <%= f.text_field :neighbourhood %><br><br>
  
  <%= f.label "Location Photo" %>
  <%= f.file_field :picture %><br><br>

  <%= f.submit %>
<% end %>